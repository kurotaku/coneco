<h2>ユーザー</h2>
<ul>
	<% @users.each do |user| %>
		<li><%= user.email %></li>
	<% end %>
</ul>

<h2>Posts</h2>
<ul>
	<% @posts.each do |post| %>
		<li><%= post.content %></li>
	<% end %>
</ul>

<%= form_for(@post) do |f| %>

  <div class="form-group">
    <%= f.label :content, '内容' %>
    <%= f.text_field :content, class: 'form-control' %>
  </div>
    
  <%= f.submit '登録する', class: 'btn btn-primary btn-block' %>
  
<% end %>

<%= link_to 'hghogwe', api_v1_posts_path %>